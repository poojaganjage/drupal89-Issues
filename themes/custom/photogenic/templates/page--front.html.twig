                    <hr class="team_hr team_hr_left hr_gray"/><span class="txt_darkgrey">{{ contact_heading }}</span>
                    <hr class="team_hr team_hr_right hr_gray"/>
                </div>
            </div>
            
            <div class="col-md-12">
                <p>{{ contact_description }}</p>
                <p><img src="{{ contact_address_icon }}" alt="icon 1" />{{ contact_address }}</p>
                <p><img src="{{ contact_phone_icon }}"  alt="icon 2" />{{ contact_phone_number }}</p>
